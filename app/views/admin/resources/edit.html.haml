- @title = "Edit Resource - #{@admin_resource.name_was}"

- @theme = "day"
- content_for :header, @admin_resource.name_was
= render "admin/tabs"
- content_for :breadcrumbs do
  %ol
    %li= link_to "resources", admin_resources_path
    %li= link_to @admin_resource.name_was.downcase, @admin_resource
    %li.breadcrumb-muted edit
- content_for :sidebar, render("admin/sidebar")

.dashboard-container
  = render "form", admin_resource: @admin_resource
